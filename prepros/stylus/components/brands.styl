.carousel
    background #EEE
    height 160px !important
.carousel-cell
    width 28%
    height 160px
    margin-right 10px
    border-radius 5px
    counter-increment carousel-cell
.carousel-cell:before
    display block
    text-align center
    line-height 160px
    font-size 80px
    color white
.carousel-cell img
  width 100%
  height 100%